	"compilerOptions": {
		"target": "es5",
		"jsx": "react",
		"reactNamespace": "app",
		"lib": ["dom", "es2015", "es5"],
		"experimentalDecorators": true,
		"sourceMap": true,
		"declaration": true,
		"declarationDir": ".",
